{
    "name": "yiisoft/yiiframework-com",
    "description": "yiiframework.com source code",
    "keywords": ["yii2", "framework"],
    "homepage": "http://www.yiiframework.com/",
    "type": "project",
    "license": "BSD-3-Clause",
    "minimum-stability": "stable",
    "require": {
        "php": ">=5.4.0",
        "yiisoft/yii2": "2.0.*@dev",
        "yiisoft/yii2-bootstrap": "2.0.*@dev",
        "bower-asset/bootstrap-sass": "3.3.6",
        "bower-asset/font-awesome": "^4.5",
        "yiisoft/yii2-swiftmailer": "2.0.*@dev",
        "yiisoft/yii2-apidoc": "2.0.*@dev",
        "yiisoft/yii2-elasticsearch": "2.0.*@dev",
        "knplabs/github-api": "~1.2",
        "yiisoft/yii2-authclient": "2.0.*@dev",
        "cebe/yii2-gravatar": "1.1",
        "scrivo/highlight.php": "8.*"
    },
    "require-dev": {
        "yiisoft/yii2-debug": "2.0.*@dev"
    },
    "config": {
        "process-timeout": 1800
    },
    "extra": {
        "asset-installer-paths": {
            "npm-asset-library": "vendor/npm",
            "bower-asset-library": "vendor/bower"
        }
    }
}
